//For loop
using System;

class Program {
  public static void Main () {
  
  int num, sum = 0;
  for (num = 24; num <= 74;)
  {
  num = num + 1;
  sum = sum + num;
  }
  Console.WriteLine("Sum = {0}", sum);
  Console.ReadLine();
  }
}

//While loop
using System;

class Program {
  public static void Main () {
  
  int num = 25;
  int sum = 0;
  while(num <= 75)
  {
    sum = num + sum;
    num = num + 1;
  }
  Console.WriteLine("Sum = "+ sum);
  Console.ReadLine();
  }
}
