 using System;

class Program {
  public static void Main (string[] args) {
    int i=10; //initialization
    while(i<20) // condition
    {

    Console.WriteLine("Hello World");
      i+=2; //increment
    }
    //Console.WriteLine("Hello World");
  }
}


using System;

class Program {
  public static void Main (string[] args) {
    int i=10; //initialization
    do
    {

    Console.WriteLine("Hello World");
      i+=2; //increment
    }
    //Console.WriteLine("Hello World");
  }
}



      
    
