using System;

class Program {
  public static void Main (string[] args) {

   int[] numbers = {1, 4, 9, 16};

   Console.WriteLine (numbers[1]);
   Console.WriteLine ("Length of the array is {0}", 
   numbers.Length);

   Console.WriteLine (numbers[2]);
   Console.WriteLine ("Length of the array is {0}", 
   numbers.Length);
    
   Console.WriteLine (numbers[3]);
   Console.WriteLine ("Length of the array is {0}", 
   numbers.Length);

   for (int i=0; i<5; i++)
   {
     Console.WriteLine (numbers[1]);
   }
  }
}
