using System;

class Program
{
    public static void Main(string[] args)
    {
        Console.WriteLine("6+8 is");
        Console.WriteLine("1. 14");
        Console.WriteLine("2. 7");
        Console.WriteLine("3. 11");
        Console.WriteLine("4. 12");
        Console.WriteLine("Answer = 14");

        int opt = Convert.ToInt32(Console.ReadLine());

        if (opt == 1)
        {
            Console.WriteLine("Correct");
        }
      else
     {
                Console.WriteLine("Incorrect");
        }
     
      
        Console.WriteLine("100-36 is");
        Console.WriteLine("1. 74");
        Console.WriteLine("2. 54");
        Console.WriteLine("3. 64");
        Console.WriteLine("4. 24");
        Console.WriteLine("Answer = 64");

        int opt1 = Convert.ToInt32(Console.ReadLine());

        if (opt1 == 3)
        {
            Console.WriteLine("Correct");
        }
      else
      {
                Console.WriteLine("Incorrect");
        }

    
        Console.WriteLine("6*9 is");
        Console.WriteLine("1. 68");
        Console.WriteLine("2. 54");
        Console.WriteLine("3. 15");
        Console.WriteLine("4. 28");
        Console.WriteLine("Answer = 54");

        int opt2 = Convert.ToInt32(Console.ReadLine());

        if (opt2 == 2)
        {
            Console.WriteLine("Correct");
        }
      else
      {
                Console.WriteLine("Incorrect");
        }


        Console.WriteLine("20*10 is");
        Console.WriteLine("1. 80");
        Console.WriteLine("2. 200");
        Console.WriteLine("3. 100");
        Console.WriteLine("4. 400");
        Console.WriteLine("Answer = 200");

        int opt3 = Convert.ToInt32(Console.ReadLine());

        if (opt3 == 2)
        {
            Console.WriteLine("Correct");
        }
      else
      {
                Console.WriteLine("Incorrect");
        }


        Console.WriteLine("10/5 is");
        Console.WriteLine("1. 3");
        Console.WriteLine("2. 4");
        Console.WriteLine("3. 5");
        Console.WriteLine("4. 2");
        Console.WriteLine("Answer = 2");

        int opt4 = Convert.ToInt32(Console.ReadLine());

        if (opt4 == 4)
        {
            Console.WriteLine("Correct");
        }
      else
      {
                Console.WriteLine("Incorrect");
        }

    }
}
        
